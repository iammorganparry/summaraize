{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "format": {
      "dependsOn": ["^format"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "install:ce": {
      "dependsOn": ["^install:ce"]
    },
    "build": {
      "dependsOn": ["^build"]
    }
  }
}
